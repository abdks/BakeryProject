@model BakeryProject.MediatorDesignPattern.Results.Contact.GetContactByIdQueryResult

@{
    ViewData["Title"] = "CreateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<link href="~/lib/bootstrap/dist/css/bootstrap.rtl.min.css" rel="stylesheet" />

<div class="container">
    <br /><br />
    <form method="post">
        <label>ID</label>
        <input type="text" asp-for="ContactID" class="form-control" />
        <br />
        <label>Adres</label>
        <input type="text" asp-for="Adress" class="form-control" />
        <br />
        <label>No</label>
        <input type="text" asp-for="No" class="form-control" />
        <br />
        <label>Mail</label>
        <input type="text" asp-for="Mail" class="form-control" />
        <br />
        <button class="btn btn-primary">Güncelle</button>
    </form>
</div>