@model List<BakeryProject.MediatorDesignPattern.Results.Contact.GetAllContactQueryResult>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}




<div class="container">
    <br /><br />
    <table class="table table-bordered">
        <tr>
            <th>#</th>

          
            <th>Adres</th>
            <th>No</th>
            <th>Mail</th>
            <th>Sil</th>
            <th>Güncelle</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ContactID</td>
             
                <td>@item.Adress</td>
                <td>@item.No</td>
                <td>@item.Mail</td>
                <td><a href="/Contact/DeleteContact/@item.ContactID" class="btn btn-danger">Sil</td>
                <td><a href="/Contact/UpdateContact/@item.ContactID" class="btn btn-danger">Güncelle</td>
            </tr>
        }
    </table>
    <a href="/Contact/CreateContact/" class="btn btn-success">Yeni İletişim Girişi</a>
</div>