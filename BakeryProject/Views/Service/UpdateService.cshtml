@model BakeryProject.CQRSPattern.Results.Service.GetServiceByIdQueryResult
@{
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<link href="~/lib/bootstrap/dist/css/bootstrap.rtl.min.css" rel="stylesheet" />

<div class="container">
    <br /><br />
    <form method="post">
        <label>Service ID</label>
        <input type="text" asp-for="ServicesID" class="form-control" />
        <br />
        <label>Service Resimi</label>
        <input type="text" asp-for="Image" class="form-control" />
        <br />
        <label>Service Başlığı</label>
        <input type="text" asp-for="Title" class="form-control" />
        <br />
        <label>Service Açıklaması</label>
        <input type="text" asp-for="Description" class="form-control" />
        <br />


        <button class="btn btn-primary">Service Güncelle</button>
    </form>
</div>